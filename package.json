    "cloudinary": "^2.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-status-codes": "^2.3.0",
    "joi": "^17.10.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mailersend": "^2.6.0",
    "mime-types": "^3.0.1",
